{
	"camelcase": true,
	"curly": true,
	"eqeqeq":true,
	"forin": true,
	"immed": true,
	"indent": 2,
	"latedef": true,
	"newcap": true,
	"noarg": true,
	"noempty": true,
	"nonbsp": true,
	"nonew": true,
	"quotmark": "single",
	"undef": true,
	"unused": true,
	"strict": true,
	"maxparams": 3,
	"maxdepth": 2,
	"maxstatements": 5,
	"maxlen": 80,
	"maxerr": 5,
	"browser": true,
	"node": true,

	"globals": {
		"describe": false,
		"it": false,
		"before": false,
		"beforeEach": false,
		"after": false,
		"afterEach": false
	}
}
